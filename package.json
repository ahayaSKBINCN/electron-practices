{
  "name": "western-upon-cloud",
  "version": "1.0.0",
  "main": "main/index.ts",
  "license": "MIT",
  "scripts": {
    "clean": "node ./snowpack-cli/bin/cli.js clean",
    "dev": "node ./snowpack-cli/bin/cli.js dev",
    "build": "node ./snowpack-cli/bin/cli.js build",
    "dist": "electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "open": "open dist/mac/${npm_package_name}.app",
    "try": "run-s clean build dist:dir open"
  },
  "build": {
    "extends": "./snowpack-cli/config/electron-builder.js"
  },
  "dependencies": {
    "electron": "^12.0.8"
  },
  "devDependencies": {
    "@snowpack/plugin-react-refresh": "^2.5.0",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "builtin-modules": "^3.2.0",
    "chalk": "^4.1.1",
    "commander": "^7.2.0",
    "concurrently": "^6.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "esbuild": "^0.12.1",
    "execa": "^5.0.0",
    "glob": "^7.1.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "rmraf": "^1.0.3",
    "signal-exit": "^3.0.3",
    "snowpack": "^3.4.0",
    "typescript": "^4.2.4",
    "@snowpack/plugin-dotenv": "^2.1.0",
    "@snowpack/plugin-typescript": "^1.2.1",
    "electron-devtools-installer": "3.2.0"
  }
}
